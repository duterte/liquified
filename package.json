{
  "name": "liquified",
  "displayName": "liquified",
  "description": "Syntax higlighting/Snippets for shopify liquid files",
  "version": "1.0.0",
  "publisher": "German Ochea",
  "keyword": [
    "shopify",
    "liquid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/duterte/liquified"
  },
  "icon": "liquified_icon.png",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "liquified",
        "path": "./snippets/liquid.json"
      }
    ],
    "grammars": [
      {
        "language": "liquified",
        "scopeName": "source.liquid",
        "path": "./syntaxes2/liquified.tmLanguage.json",
        "injectTo": [
          "source.liquid"
        ]
      }
    ],
    "languages": [
      {
        "id": "liquified",
        "aliases": [
          "liquid",
          "liquified"
        ],
        "extensions": [
          ".liquid"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^3.14.1"
  }
}
